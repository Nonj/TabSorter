{
    "manifest_version": 2,
    "name": "Tab Sorter",
    "version": "0.1",
    "permissions": [
      "tabs"
    ],
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["./src/content.js", "./src/popup.js"]
      }
    ],
    "browser_action": {
      "default_icon": "./icon/icon.png",
      "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["./src/background.js"]
    }
  }